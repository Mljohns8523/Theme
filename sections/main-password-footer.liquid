<style>
  @import url('https://fonts.googleapis.com/css2?family=DM+Sans:wght@400;700&display=swap');

  .password__footer {
    font-family: 'DM Sans', sans-serif;
    background: #000000;
    color: #fff;
    text-align: center;
    padding: 24px 16px;
    margin: 0 auto;
  }

  .list-social {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    gap: 12px;
    margin: 0 0 16px;
    padding: 0;
  }

  .list-social__item {
    list-style: none;
  }

  .list-social__link {
    display: inline-block;
    width: 24px;
    height: 24px;
    color: #FFFFFF;
    text-decoration: none;
  }

  .list-social__link .svg-wrapper {
    display: block;
    width: 100%;
    height: 100%;
  }

  .list-social__link:focus {
    outline: 2px solid #003e80;
    outline-offset: 2px;
    border-radius: 4px;
  }

  .password__titan-branding {
    font-size: 16px;
    font-weight: 400;
    color: #FFFFFF;
    margin: 0 0 16px;
    line-height: 1.5;
  }

  .password__titan-branding img.titan-logo {
    height: 20px;
    vertical-align: middle;
    margin-left: 6px;
  }

  .password__footer-text {
    font-size: 14px;
    color: #CCCCCC;
    display: block;
    margin: 0;
  }

  .password__footer-text a {
    color: #CCCCCC;
    text-decoration: none;
  }

  .password__footer-text a:focus {
    text-decoration: underline;
    color: #003e80;
  }

  @media (max-width: 768px) {
    .password__footer {
      padding: 16px 12px;
    }

    .list-social {
      gap: 10px;
      margin-bottom: 12px;
    }

    .list-social__link {
      width: 20px;
      height: 20px;
    }

    .password__titan-branding {
      font-size: 13px;
    }

    .password__titan-branding img.titan-logo {
      height: 16px;
      margin-left: 4px;
    }

    .password__footer-text {
      font-size: 12px;
    }
  }

  @media (max-width: 480px) {
    .password__footer {
      max-width: 95%;
    }

    .list-social {
      gap: 8px;
    }
  }
</style>

{% schema %}
{
  "name": "t:sections.main-password-footer.name",
  "settings": [
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "label": "t:sections.all.colors.label",
      "default": "scheme-1"
    }
  ]
}
{% endschema %}
